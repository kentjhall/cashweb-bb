{

  "manifest_version": 2,
  "name": "CashWeb BB",
  "version": "1.0",

  "description": "CashWeb client for Chrome.",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cashlinkify.js"],
      "run_at": "document_idle"
    }
  ],

  "browser_action": {
    "default_icon": "favicon-32x32.png",
    "default_title": "CashWeb BB",
    "default_popup": "cashbb.html",
    "browser_style": true
  },

  "permissions": ["storage", "downloads"]

}
